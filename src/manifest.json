{
  "name": "ws hand shaker",
  "description": "Init ws chats",
  "version": "0.0.1",
  "manifest_version": 3,
  "icons": {
    "16": "./icon-16.png",
    "32": "./icon-32.png",
    "48": "./icon-48.png",
    "128": "./icon-128.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ],
  "host_permissions": [
    "*://*/*",
    "<all_urls>"
  ]
}

